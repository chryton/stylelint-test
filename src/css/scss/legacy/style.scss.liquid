/* ************************************* GLOBAL LAYOUT ************************************* */

#wrapper {
  overflow: hidden;
}

.container {
  padding: 0;
}

.page_container {
  max-width: 1020px;
  margin: 0 auto;
}

.page_container_main {};

#showcase {
  margin: 15px 0 0;
}

#main {
  clear: both;
  overflow: hidden;
  padding: 0 !important;

}
.template-index #main {
  }

#main_content,
.column .widget {
  margin-bottom: 30px !important;
}

#bottom {
  background: unquote("{{ settings.color_2 }}");
  border: 1px solid unquote("{{ settings.color_1 }}");
  border-top: none;
}

.bottom_content {
  padding-bottom: 5px;
  -webkit-box-shadow: inset 0 0 60px 0 #c3c1c1;
  box-shadow: inset 0 0 60px 0 #c3c1c1;
}

